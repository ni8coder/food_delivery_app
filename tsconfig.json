{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*" : ["src/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["src/components/*"],
      "@feature/*": ["src/feature/*"],
      "@models/*": ["src/models/*"],
      "@navigators/*": ["src/navigators/*"],
      "@screens/*": ["src/screens/*"],
      "@theme/*": ["src/theme/*"],
      "@app/*": ["src/app/*"],
      "@config/*": ["src/config/*"],
      "@utils/*": ["src/utils/*"],
      "@helpers/*": ["src/helpers/*"],
    }
  }
}
