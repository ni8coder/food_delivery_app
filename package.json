{
  "name": "food_delivery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "npx react-native setup-ios-permissions && npx pod-install",
    "ios:dev": "ENVFILE=.env.development react-native run-ios --simulator='iPhone 14'",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios --simulator='iPhone 14'",
    "ios:production": "ENVFILE=.env.production react-native run-ios --simulator='iPhone 14'",
    "android:staging": "cd android && ./gradlew clean && cd .. && npx react-native run-android --variant=stagingDebug --appIdSuffix 'staging'",
    "android:staging-release": "cd android && ./gradlew clean && cd .. && npx react-native run-android --variant=stagingRelease --appIdSuffix 'staging'",
    "android:dev": "npx react-native run-android --variant=developmentDebug --appIdSuffix 'development'",
    "android:dev-release": "cd android && ./gradlew clean && cd .. && npx react-native run-android --variant=developmentRelease --appIdSuffix 'development'",
    "android:prod": "cd android && ./gradlew clean && cd .. && npx react-native run-android --variant=productionDebug --appIdSuffix ''",
    "android:prod-release": "cd android && ./gradlew clean && cd .. && npx react-native run-android --variant=productionRelease --appIdSuffix ''"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.0",
    "@react-native-async-storage/async-storage": "^1.19.2",
    "@react-native-firebase/analytics": "^18.3.0",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/auth": "^18.3.1",
    "@react-native-firebase/firestore": "^18.3.1",
    "@react-native-firebase/messaging": "^18.3.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/pubnub": "^7.3.2",
    "axios": "^1.5.0",
    "i18next": "^23.5.1",
    "intl-pluralrules": "^2.0.1",
    "patch-package": "^8.0.0",
    "pubnub": "^7.3.3",
    "pubnub-react": "^3.0.2",
    "react": "18.2.0",
    "react-i18next": "^13.2.2",
    "react-native": "0.72.4",
    "react-native-actions-sheet": "0.8.21",
    "react-native-animated-dots-carousel": "^1.0.2",
    "react-native-config": "^1.5.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fbsdk-next": "^12.1.0",
